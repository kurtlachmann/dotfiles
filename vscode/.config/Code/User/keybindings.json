// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+,",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+7",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k shift+8",
        "command": "extension.selectParenthesis"
    },
    {
        "key": "ctrl+k shift+9",
        "command": "-extension.selectParenthesis"
    },
    {
        "key": "ctrl+k shift+9",
        "command": "extension.selectParenthesisOuter"
    },
    {
        "key": "ctrl+k shift+0",
        "command": "-extension.selectParenthesisOuter"
    },
    {
        "key": "ctrl+k 8",
        "command": "extension.selectSquareBrackets"
    },
    {
        "key": "ctrl+k ctrl+alt+8",
        "command": "-extension.selectSquareBrackets"
    },
    {
        "key": "ctrl+k 9",
        "command": "extension.selectSquareBracketsOuter"
    },
    {
        "key": "ctrl+k ctrl+alt+9",
        "command": "-extension.selectSquareBracketsOuter"
    },
    {
        "key": "ctrl+k 7",
        "command": "extension.selectCurlyBrackets"
    },
    {
        "key": "ctrl+k ctrl+alt+7",
        "command": "-extension.selectCurlyBrackets"
    },
    {
        "key": "ctrl+k 0",
        "command": "extension.selectCurlyBracketsOuter"
    },
    {
        "key": "ctrl+k ctrl+alt+0",
        "command": "-extension.selectCurlyBracketsOuter"
    },
    {
        "key": "alt+left",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+alt+-",
        "command": "-workbench.action.navigateBack"
    },
    {
        "key": "alt+right",
        "command": "workbench.action.navigateForward"
    },
    {
        "key": "ctrl+shift+-",
        "command": "-workbench.action.navigateForward"
    },
    {
        "key": "alt+i",
        "command": "workbench.action.focusPanel",
        "when": "editorTextFocus"
    },
    {
        "key": "alt+i",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+i",
        "command": "-editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+i",
        "command": "-focusSuggestion",
        "when": "suggestWidgetVisible && textInputFocus && !config.editor.suggest.selectQuickSuggestions && !suggestWidgetHasFocusedSuggestion"
    },
    {
        "key": "ctrl+i",
        "command": "-toggleSuggestionDetails",
        "when": "suggestWidgetHasFocusedSuggestion && suggestWidgetVisible && textInputFocus"
    },
    {
        "key": "ctrl+i",
        "command": "editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "f12",
        "command": "-editor.action.revealDefinition",
        "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
        "key": "ctrl+i",
        "command": "editor.gotoNextSymbolFromResult",
        "when": "hasSymbols"
    },
    {
        "key": "f12",
        "command": "-editor.gotoNextSymbolFromResult",
        "when": "hasSymbols"
    },
    {
        "key": "ctrl+i",
        "command": "goToNextReference",
        "when": "inReferenceSearchEditor || referenceSearchVisible"
    },
    {
        "key": "f12",
        "command": "-goToNextReference",
        "when": "inReferenceSearchEditor || referenceSearchVisible"
    }
]
