URxvt*termName: rxvt-256color

!URxvt.font:     xft:DejaVu sans mono:size=9
!URxvt*boldFont: xft:DejaVu sans mono:size=9:style=Bold

!*font:       -kakwa-kakwafont-medium-r-normal--0-0-72-72-c-0-iso8859-1
!*boldFont:   -kakwa-kakwafont-medium-r-normal--0-0-72-72-c-0-iso8859-1
!*italicFont: -kakwa-kakwafont-medium-r-normal--0-0-72-72-c-0-iso8859-1

*font:       -windows-proggytiny-medium-r-normal--10-80-96-96-c-60-iso8859-1
*boldFont:   -windows-proggytiny-medium-r-normal--10-80-96-96-c-60-iso8859-1
*italicFont: -windows-proggytiny-medium-r-normal--10-80-96-96-c-60-iso8859-1


URxvt.scrollBar:  false
URxvt.shading:    30
URxvt.internalBorder: 20
! URxvt.letterSpace: -1

URxvt*lineSpace: 2


*background: #282828
*foreground: #EAE0C7
! black
*color0: #3F3F3F
